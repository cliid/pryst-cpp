// Test file for function declarations and calls
function add(int a, int b) -> int {
    return a + b;
}

function greet(str name) -> str {
    return "Hello, " + name;
}

// Function with complex types
function processMap(map<str,int> data) -> int {
    return data["value"];
}

// Lambda function
let multiply = (int x, int y) -> int {
    return x * y;
};

// Test function calls
print(add(5, 3));
print(greet("World"));

let testMap = new map<str,int>();
testMap["value"] = 42;
print(processMap(testMap));
print(multiply(6, 7));
