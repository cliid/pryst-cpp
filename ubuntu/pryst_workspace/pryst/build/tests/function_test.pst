// Test file for function declarations and calls
func add(a: int, b: int): int {
    return a + b;
}

func greet(name: str): str {
    return "Hello, " + name;
}

// Function with complex types
func processMap(data: map<str,int>): int {
    return data["value"];
}

// Lambda function
var multiply: func(int, int): int = func(x: int, y: int): int {
    return x * y;
};

// Test function calls
func main(): void {
    print(add(5, 3));
    print(greet("World"));

    var testMap: map<str,int> = {"value": 42};
    print(processMap(testMap));
    print(multiply(6, 7));
}
